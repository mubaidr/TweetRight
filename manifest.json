{
	"background": {
      "persistent": false,
      "scripts": [ "script.js" ]
   },
   "browser_action": {
      "default_icon": "static/32.png",
      "default_title": "Share on Twitter"
   },
   "description": "Share web pages, selected text and links on Twitter using the toolbar button, keyboard command and context menu option.",
   "homepage_url": "https://browsernative.com/share-on-twitter-chrome-extension/",
   "icons": {
      "128": "static/128.png",
      "16": "static/16.png",
      "32": "static/32.png",
      "48": "static/48.png"
   },
   "manifest_version": 2,
   "name": "Share on Twitter",
   "permissions": [ "contextMenus", "activeTab" ],
   "version": "1.9",
   "commands": {
     "_execute_browser_action": {
       "suggested_key": {
         "default": "Alt+T"
       }
     }
   }
}
